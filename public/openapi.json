{
  "openapi": "3.0.0",
  "info": {
    "title": "BaseApp Next.js Fullstack Template",
    "version": "1.0.0",
    "description": "This is the OpenAPI specification for your project."
  },
  "servers": [
    {
      "url": "http://localhost:3000/api",
      "description": "Local development server"
    }
  ],
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "MeResponse": {
        "type": "object",
        "properties": {
          "user": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "email": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "image": {
                "type": "string",
                "nullable": true
              }
            },
            "required": [
              "id",
              "email",
              "name"
            ]
          }
        },
        "required": [
          "user"
        ]
      }
    },
    "responses": {}
  },
  "outputDir": "./public",
  "paths": {
    "/me": {
      "get": {
        "operationId": "get-me",
        "summary": "Get current user information",
        "description": "Fetches current user information",
        "tags": [
          "Me"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MeResponse"
                }
              }
            }
          }
        }
      }
    }
  }
}